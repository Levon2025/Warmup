#include <stdio.h>


int main(){
      int arr1[] = { 1,2,3};
      int arr2[] = {4,5,6,};
      int size = sizeof(arr1) / sizeof(arr1[0]);

      for(int i = 0; i < size  * 2; i++){
            if(i < size){
           printf("%d " ,arr1[i]);
           }else{
               printf("%d ",arr2[i - size]);
              }

           }
           return 0;
}
