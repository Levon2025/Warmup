#include <stdio.h>

int main(){
      char str[100];
      printf("Input the Elements ");
      scanf("%s",str);
      for(int i = 0; str[i] != '\0';i++){
          if(str[i] >= 'a' && str[i] <= 'z'){
               str[i] = str[i] - 32;
               }
      }
      printf("Big leeter %s\n",str);
      return 0;
}
