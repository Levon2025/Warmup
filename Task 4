#include <stdio.h>

int main(){
        char str[100];
        printf("Enter the Elements");
        scanf("%s",str);
        printf("Values is array");
        for(int i = 0;str[i] != '0' ; i++){
              if(str[i] >= '0' && str[i] <= '9'){
                  printf ("%c " ,str[i]);
                 }
        }
        printf("\n");
        return 0;
}
