#include <stdio.h>

int main(){
        int arr1[] = {1,2,3};
        int arr2[] = {4,5,6};
        int size = sizeof(arr1) / sizeof(arr1[0]);
        for(int i = size * 2 - 1; i >= 0 ; i--){
            if(i >= size){
                printf("%d " ,arr1[i - size]);
              }else{
                  printf("%d ", arr2[i]);
              }
             }
             return 0;
}
