#include <stdio.h>


int Equal(int arr1[],int arr2[],int size){
    for(int i = 0; i < size;i++){
        if(arr1[i] != arr2[i]){
            return 0;
        }
       }
       return 1;
      }
int main(){
    int arr1[] = {1,2,3,4,5};
    int arr2[] ={1,2,3,4,5};
    int size = sizeof(arr1) / sizeof(arr1[0]);
    if(Equal(arr1,arr2,size)){
        printf(" They are same  ");
      }else{
          printf("They are diferent");
         }
         return 0;
}
