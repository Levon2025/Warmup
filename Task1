#include <stdio.h>


int main(){
        int n;
        printf("Enter the size of array");
        scanf("%d",&n);
        int a[n];
        int b[n];
        printf("Fisrt element of array %d\n",n);
        for(int i = 0; i < n ; i ++){
               printf("a[%d] = ",i);
               scanf("%d",&a[i]);
        }
        printf("Second element of array %d\n",n);
        for(int i = 0; i < n; i++){
              printf("b[%d] = " ,i);
              scanf("%d",&b[i]);
        }
        printf("Elements\n");
        for(int i = 0; i < n; i++){
              printf("a[%d] * b[%d] = %d\n",i ,i, a[i] * b[i]);
        }
        return 0;
}

